@article{Zaina2012,
abstract = {O uso de sistemas de recomenda{\c{c}}{\~{a}}o com a finalidade de melhorar a intera{\c{c}}{\~{a}}o usu{\'{a}}rio-computador tem crescido em aplica{\c{c}}{\~{o}}es eletr{\^{o}}nicas como e-commerce e e-learning. Sistemas de recomenda{\c{c}}{\~{a}}o visam sugerir informa{\c{c}}{\~{o}}es e servi{\c{c}}os que possam ser atrativos baseando-se no uso automatizado de dados que descrevem as prefer{\^{e}}ncias do usu{\'{a}}rio. Na {\'{a}}rea educacional, a recomenda{\c{c}}{\~{a}}o de conte{\'{u}}dos mais relevantes pode atrair a aten{\c{c}}{\~{a}}o do estudante, motivando-o durante o processo de ensino-aprendizagem. Uma das formas de se alcan{\c{c}}ar este objetivo se d{\'{a}} por meio do processamento computacional de dados que descrevem o estilo de aprendizagem do estudante. Neste contexto, o objetivo deste trabalho {\'{e}} apresentar a metodologia e-LORS, uma abordagem para a recomenda{\c{c}}{\~{a}}o de conte{\'{u}}do eletr{\^{o}}nico baseada no relacionamento entre perfis e objetos de aprendizagem. Na metodologia proposta, perfis de aprendizagem s{\~{a}}o descritos por dimens{\~{o}}es discretas de maneira a atender diferentes perspectivas de prefer{\^{e}}ncia do estudante. J{\'{a}} a metodologia de recomenda{\c{c}}{\~{a}}o usa estas dimens{\~{o}}es para filtrar os objetos de aprendizagem – descritos pelo padr{\~{a}}o IEEE LOM – mais adequados ao estudante.},
author = {Zaina, Luciana A M and Bressan, Gra{\c{c}}a and Cardieri, Maria Ang{\'{e}}lica A. C. and {Rodrigues J{\'{u}}nior}, Jos{\'{e}} Fernando},
doi = {10.5753/RBIE.2012.20.1.04},
file = {:C$\backslash$:/Users/Jeffersonro/Documents/Mestrado - Jefferson Rodrigues/Artigos/Scratch e Games na educa{\c{c}}{\~{a}}o/11 - E-LORS Uma Abordagem para Recomenda{\c{c}}{\~{a}}o de Objetos de Aprendizagem.pdf:pdf},
issn = {1414-5685},
journal = {Revista Brasileira de Inform{\'{a}}tica na Educa{\c{c}}{\~{a}}o},
keywords = {aprendizagem eletr{\^{o}}nica,objeto de aprendizagem,padr{\~{a}}o IEEE LOM,perfil de aprendizagem,sistemas de recomenda{\c{c}}{\~{a}}o},
