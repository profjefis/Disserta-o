@article{Bastos2010,
abstract = {Resumo: Uns dos grandes desafios para o ensino atual {\'{e}} a inclus{\~{a}}o da tecnologia no seu dia-a-dia. Muitos a criticam, no entanto, seu uso adequado pode proporcionar muitos benef{\'{i}}cios no que tange {\`{a}} aprendizagem. Este artigo trata como o Construcionismo pode ser usado no contexto educacional utilizando como ferramentas a linguagem de programa{\c{c}}{\~{a}}o Scratch e a placa eletr{\^{o}}nica Arduino. Palavras-chave: Construcionismo, Scratch, Arduino. 1. Introdu{\c{c}}{\~{a}}o. Em muitos casos se observa que quanto mais abstrato determinado assunto, mais dif{\'{i}}cil {\'{e}} para o aluno assimilar tal conhecimento (DALE, 1969). Por exemplo: " ax 2 + bx + c " {\'{e}} um conceito extremamente importante, pois atrav{\'{e}}s das equa{\c{c}}{\~{o}}es do segundo grau os f{\'{i}}sicos conseguem prever acelera{\c{c}}{\~{o}}es, quedas livres, dentre outros fen{\^{o}}menos da natureza. Contudo, para quem v{\^{e}} pela primeira vez, isso pode parecer muito abstrato, n{\~{a}}o tendo qualquer aplica{\c{c}}{\~{a}}o pr{\'{a}}tica na sua vida pessoal, apenas para o dia da prova. Em muitas escolas, ainda, um dos grandes problemas do ensino {\'{e}} o seu distanciamento do cotidiano dos alunos. Os estudantes muitas vezes t{\^{e}}m prefer{\^{e}}ncia por uma mat{\'{e}}ria ou outra, mas s{\~{a}}o obrigados a cursar o curr{\'{i}}culo geral do curso. Aqueles conte{\'{u}}dos de mat{\'{e}}rias que julgam dif{\'{i}}ceis, ou que n{\~{a}}o gostam, s{\~{a}}o aproveitados somente},
author = {Bastos, Bruno Leal and Borges, Marcos and D'Abreu, Jo{\~{a}}o},
file = {:C$\backslash$:/Users/Jeffersonro/Documents/Mestrado - Jefferson Rodrigues/Artigos/Scratch e Games na educa{\c{c}}{\~{a}}o/5 - Scratch, arduino e o construcionismo ferramentas.pdf:pdf},
isbn = {9788598429021},
journal = {Semin{\'{a}}rio de Tecnologia Educacional de Arauc{\'{a}}ria},
pages = {1--10},
title = {{Scratch, Arduino e o Construcionismo: Ferramentas para a Educa{\c{c}}ao}},
year = {2010}
}
